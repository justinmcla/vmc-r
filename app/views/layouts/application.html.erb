<!DOCTYPE html>
<html>
  <head>
    <title>Venue Management Console</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'bootstrap' %>
    <%= yield :stylesheet %>
    <link href="https://fonts.googleapis.com/css2?family=Lato&family=Lexend+Deca&family=Lexend+Giga&display=swap" rel="stylesheet">
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <%= content_for?(:framing) ? yield(:framing) : yield %>
  </body>
</html>
