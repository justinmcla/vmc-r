<%= render partial: 'shared/errors', locals: { resource: @inventory } %>
<%= form_for [ :admin, @venue, @inventory ] do |f| %>
  <%= content_tag(:div, class: 'row') do %>
    <%= content_tag(:div, class: 'form-group col-lg-6') do %>
      <%= f.label :name, "Inventory Name" %>
      <%= f.text_field :name, class: 'form-control', required: true %>
    <% end %>
  <% end %>
  <br>
  <%= f.submit class: 'btn' %>
<% end %>