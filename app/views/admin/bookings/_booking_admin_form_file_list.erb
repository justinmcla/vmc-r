<%= form_with url: admin_attachment_path, method: 'delete' do |f| %>
  <%= f.collection_check_boxes :files, @booking.files, :id, :filename, {}, {} do |builder| %>
    <%= builder.check_box %> <%= builder.label %> <%= link_to 'View', rails_blob_path(builder.object, disposition: 'inline') %><br>
  <% end %>
  <br>
  <%= f.submit 'Delete File(s)', class: 'btn' %>
<% end %>