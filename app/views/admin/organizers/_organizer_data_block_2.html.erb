<div class = 'col'>

  <h3>Contact Persons</h3>

  <% @organizer.point_of_contacts.each do |poc| %>

    <p><%= poc.name %><br>

    <% poc.contacts.each do |contact| %>

      <%= contact.via %>: <%= contact.value %><br>

    <% end %>

    <%= link_to "Delete", admin_organizer_point_of_contact_path(@organizer, poc), method: 'delete' %>

    </p>

  <% end %>

  <br>
  
</div>