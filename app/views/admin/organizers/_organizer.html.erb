<%= render partial: 'shared/errors', locals: { resource: @organizer } %>
<%= content_tag(:div, class: 'container') do %>
  <%= form_for [ :admin, @organizer ] do |f| %>
    <%= content_tag(:div, class: 'form-row') do %>
      <%= content_tag(:div, class: 'form-group col-lg-3') do %>
        <%= f.label :name, "Organization Name" %>
        <%= f.text_field :name, class: 'form-control' %>
      <% end %>
      <%= content_tag(:div, class: 'form-group col-lg-3') do %>
        <%= f.label :url, "Website" %>
        <%= f.text_field :url, class: 'form-control' %>
      <% end %>
    <% end %>
    <%= render partial: 'shared/address', locals: { resource: @organizer, f: f } %>
    <%= render partial: 'shared/contact', locals: { f: f } %>
    <%= render partial: 'shared/point_of_contact', locals: { f: f} %>
    <br>
    <%= f.submit class: 'btn' %>
  <% end %>
<% end %>