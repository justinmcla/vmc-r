<h1><%= @organizer.name %></h1>
<% if @organizer.address %>
    <p><%= @organizer.address.address_1 %><br>
    <% if !@organizer.address.address_2.blank? %>
        <%= @organizer.address.address_2 %>
        <br>
    <% end %>
    <%= @organizer.address.city %>, <%= @organizer.address.state %> <%= @organizer.address.zip %><br>
<% end %>
<% @organizer.contacts.each do |contact| %>
    <p><%= contact.via %>: <%= contact.value %><br>
    <%= link_to "Delete", admin_organizer_contact_path(@organizer, contact), method: 'delete' %></p>
<% end %>

<h3>Contact Persons</h3>
<% @organizer.point_of_contacts.each do |poc| %>
    <p><%= poc.name %><br>
    <% poc.contacts.each do |contact| %>
        <%= contact.via %>: <%= contact.value %><br>
    <% end %>
    <%= link_to "Delete", admin_organizer_point_of_contact_path(@organizer, poc), method: 'delete' %>
    </p>
<% end %>
<br>
<%= button_to "Edit Organizer", edit_admin_organizer_path(@organizer), method: 'get', class: 'btn' %> 